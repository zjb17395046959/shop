<view class="my"><view class="my-nav"><block a:if="{{!token}}"><image src="../../static/img/16.jpg" mode="widthFix"></image></block><block a:if="{{token}}"><image src="{{img_url}}" mode="widthFix"></image></block><block a:if="{{!token}}"><button class="my-nav-title wx-title" open-type="getAuthorize" scope="userInfo" data-event-opts="{{[['tap',[['getAuthorize',['$event']]]]]}}" onTap="__e">未登入，点击登入</button></block><block a:if="{{token}}"><view class="my-nav-box"><view>{{"用户:"+userName}}</view><view>称号：九品茶杰</view></view></block><view data-event-opts="{{[['tap',[['wx_sheZhi',['$event']]]]]}}" class="iconfont icon-shezhi" onTap="__e"></view></view><view class="my-list-nav"><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="id"><view data-event-opts="{{[['tap',[['nav',[index]]]]]}}" class="my-list-nav-box" onTap="__e"><view class="{{(('iconfont my-list-nav-box-icon')+' '+('icon-'+item.$orig.icon))}}" style="{{'color:'+(item.m0)+';'}}"></view><view class="my-list-nav-box-title">{{item.$orig.title}}</view></view></block></view><view class="my-list"><block a:for="{{my_list}}" a:for-item="item" a:for-index="index" a:key="id"><view class="my-list-box" hover-class="my-list-active" data-event-opts="{{[['tap',[['list_nav',[index]]]]]}}" onTap="__e"><view class="{{(('icon iconfont')+' '+('icon-'+item.icon))}}"></view><view class="my-list-box-title">{{item.title}}</view><image class="my-list-box-img" src="{{item.img}}" mode="widthFix"></image></view></block></view></view>