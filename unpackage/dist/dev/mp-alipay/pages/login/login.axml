<view class="login"><image class="login-img" src="../../static/img/16.jpg" mode="widthFix"></image><view hidden="{{!(iShow)}}" class="login-input"><view class="login-input-box"><input type="number" placeholder="请输入账号" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" onInput="__e"/><image hidden="{{!(userName.length!='')}}" class="login-img1" src="../../static/img/error.png" mode="scaleToFill" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" onTap="__e"></image></view><view class="login-input-box"><block a:if="{{!isPass}}"><input type="text" placeholder="请输入密码(字母+数字)" maxlength="14" password="{{true}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" onInput="__e"/></block><block a:if="{{isPass}}"><input type="text" placeholder="请输入密码(字母+数字)" maxlength="14" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" onInput="__e"/></block><image hidden="{{!(password.length!='')}}" class="login-img1" src="../../static/img/error.png" mode="scaleToFill" data-event-opts="{{[['tap',[['err_pas',['$event']]]]]}}" onTap="__e"></image><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{(('icon iconfont icon-yanjing')+' '+(isPass?'isPass':''))}}" onTap="__e"></text></view></view><view hidden="{{!(!iShow)}}" class="login-input"><view class="login-input-box"><input type="text" placeholder="请输入手机号" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','iponeNumber','$event',[]]]]]]}}" value="{{iponeNumber}}" onInput="__e"/><image hidden="{{!(iponeNumber.length!='')}}" class="login-img1" src="../../static/img/error.png" mode="scaleToFill" data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" onTap="__e"></image></view><view class="login-input-box"><input type="text" placeholder="请输入验证码" maxlength="6" data-event-opts="{{[['input',[['__set_model',['','yzm','$event',[]]]]]]}}" value="{{yzm}}" onInput="__e"/><text data-event-opts="{{[['tap',[['getYzm',['$event']]]]]}}" class="yzm" onTap="__e">{{yzmTitle}}</text></view></view><block a:if="{{iShow}}"><button class="btn" style="{{'background:'+(disable?'#eee':'')+';'}}" type="default" disable="{{disable}}" data-event-opts="{{[['tap',[['btn',[1]]]]]}}" onTap="__e">登录</button></block><block a:if="{{!iShow}}"><button class="btn" style="{{'background:'+(disable?'#eee':'')+';'}}" type="default" disable="{{disable}}" data-event-opts="{{[['tap',[['btn',[2]]]]]}}" onTap="__e">注册</button></block><block a:if="{{iShow}}"><view data-event-opts="{{[['tap',[['qh',['$event']]]]]}}" class="zc" onTap="__e">注册/验证码登录</view></block><block a:else><view data-event-opts="{{[['tap',[['qh',['$event']]]]]}}" class="zc" onTap="__e">账户密码登录</view></block></view>