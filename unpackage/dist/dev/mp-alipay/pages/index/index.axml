<view class="index"><view class="index-input-search"><input type="text" placeholder="碧螺春" placeholder-class="input-active" data-event-opts="{{[['focus',[['focus',['$event']]]]]}}" onFocus="__e"/></view><view class="list-box"><scroll-view style="height:100%;" scroll-y="true" data-event-opts="{{[['scrollToLower',[['sl_jz',['$event']]]]]}}" onScrollToLower="__e"><view class="index-swiper"><swiper style="height:100%;" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="true" indicator-active-color="red" indicator-color="white"><block a:for="{{index_swiper}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item><view class="swiper-item"><image class="img" src="{{item}}" mode="widthFix"></image></view></swiper-item></block></swiper></view><view class="index-shop-list"><block a:for="{{index_list}}" a:for-item="item" a:for-index="index" a:key="id"><view data-event-opts="{{[['tap',[['go_shop',['$0'],[[['index_list','id',item.id]]]]]]]}}" class="list-wrap" onTap="__e"><image src="{{item.img}}" mode="scaleToFill"></image><view class="list-wrap-title">{{item.title}}</view><view class="list-wrap-price"><text>{{"￥"+item.price}}</text><block a:if="{{item.oldPrice}}"><text>{{"￥"+item.oldPrice}}</text></block></view><view class="list-wrap-label"><block a:if="{{item.hot==1}}"><text>热销</text></block><block a:if="{{item.isYou==1}}"><text>包邮</text></block></view></view></block></view><page-sl vue-id="8deb4120-1" sl="{{sl}}" onVueInit="__l"></page-sl></scroll-view></view></view>